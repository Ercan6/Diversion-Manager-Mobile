{
  "expo": {
    "name": "Advent Diversion Manager",
    "slug": "AdventDiversionManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "assetBundlePatterns": [
      "assets/adaptive-icon.png",
      "assets/splash.png"
    ],
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f2f2",
      "ios": {
        "supportsTablet": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f2f2f2"
      },
      "package": "com.advent.diversionmanager"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "e3898488-0104-4a31-b980-e9858b1e5546"
      }
    },
    "ios": {
      "bundleIdentifier": "com.advent.diversionmanager"
    }
  }
}
